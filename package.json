{
    "name": "notes-ui",
    "version": "1.0.0",
    "scripts": {
        "build": "NODE_ENV=production && npm run clean && parcel build index.html --cache-dir .cache --out-dir .build --target browser",
        "clean": "npm run clean:build && npm run clean:cache",
        "clean:build": "rm -rf .build",
        "clean:cache": "rm -rf .cache",
        "start": "NODE_ENV=development && node server.js"
    },
    "dependencies": {
        "body-parser": "^1.19.0",
        "compression": "^1.7.4",
        "core-js": "^2.6.9",
        "dayjs": "^1.8.14",
        "express": "^4.17.1",
        "history": "^4.9.0",
        "puppeteer": "^1.17.0",
        "raf": "^3.4.1",
        "react": "^16.8.6",
        "react-dom": "^16.8.6",
        "react-router-dom": "^5.0.1",
        "regenerator-runtime": "^0.13.2",
        "showdown": "^1.9.0",
        "striptags": "^3.2.0"
    },
    "devDependencies": {
        "@babel/core": "^7.4.5",
        "@babel/plugin-proposal-class-properties": "^7.4.4",
        "@babel/preset-env": "^7.4.5",
        "parcel-bundler": "^1.12.3",
        "postcss-custom-properties": "^8.0.10",
        "postcss-import": "^12.0.1",
        "postcss-nested": "^4.1.2",
        "prop-types": "^15.7.2",
        "tailwindcss": "^1.0.4"
    },
    "alias": {
        "app": "./source"
    },
    "author": "Christopher Pitt <cgpitt@gmail.com>",
    "license": "MIT"
}
